fontSetUpper()
	font-size 3.8vw
	font-weight 900
	-text-stroke 1%
	text-shadow 0 0 1px rgba(51,51,51,0.2)2
	font-family Bebas
	word-spacing 15px
	letter-spacing 2px

fontSetFiraSansMedium()
	-text-stroke 1%
	text-shadow 0 0 1px rgba(51,51,51,0.2)2
	font-family FiraSansMedium
	letter-spacing 1px

fontSetFiraSansSimebold()
	text-shadow 0 0 1px rgba(51,51,51,0.2)2
	font-family FiraSansSemibold
	letter-spacing 1px

fontSetGothamLight()
	font-weight normal
	word-spacing 0vh
	line-height 2.1vh
	text-shadow 0 0 1px rgba(51,51,51,0.2)2
	font-family GothamNarrowLight

backgroundMainSet()
	width 101%
	height 101%

backgroundSet()
	position abs
	width 100%
	height 100%